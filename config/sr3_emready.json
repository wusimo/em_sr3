{
    "name": "emready",
    "phase": "train",
    "gpu_ids": [
        0
    ],
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": null
    },
    "datasets": {
        "train": {
            "name": "test",
            "mode": "HR",
            "dataroot": "dataset",
            "datatype": "mrc",
            "l_resolution": 32,
            "r_resolution": 32,
            "batch_size": 4,
            "num_workers": 8,
            "use_shuffle": true,
            "data_len": -1
        },
        "val": {
            "name": "test",
            "mode": "HR",
            "dataroot": "dataset_val",
            "datatype": "mrc",
            "l_resolution": 32,
            "r_resolution": 32,
            "batch_size": 1,
            "data_len": 50
        }
    },
    "model": {
        "which_model_G": "sr3",
        "finetune_norm": false,
        "unet": {
            "in_channel": 2,
            "out_channel": 1,
            "inner_channel": 10,
            "norm_groups": 2,
            "channel_multiplier": [
                1,
                2
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 4,
            "dropout": 0.2
        },
        "beta_schedule": {
            "train": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "val": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 128,
            "channels": 1,
            "conditional": true
        }
    },
    "train": {
        "n_iter": 20000,
        "val_freq": 100,
        "save_checkpoint_freq": 100,
        "print_freq": 20,
        "optimizer": {
            "type": "adam",
            "lr": 1e-4
        },
        "ema_scheduler": {
            "step_start_ema": 500,
            "update_ema_every": 1,
            "ema_decay": 0.9999
        }
    },
    "wandb": {
        "project": "sr_ffhq"
    }
}